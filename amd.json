  "postName": {
    "locator": "//h1",
    "type": "xpath"
  },
  "jobLocation": {
    "locator": "(//li)[16]",
    "type": "xpath"
  },
  "experience": {
    "locator": "NA",
    "type": "xpath"
  },
  "jobDescription": {
    "locator": "//article[@id=\"description-body\"]",
    "type": "xpath"
  },
  "jobURL": {
    "locator": "//*/span[1]/mat-panel-title/p[1]/a",
    "type": "xpath"
  },
  "pageJobsCount": {
    "locator": "//*[@id=\"mat-select-value-13\"]",
    "type": "xpath",
    "hasCountInText": "true"
  },
  "nextPage": {
    "locator": "(//button)[8]",
    "type": "xpath"
  }
}
