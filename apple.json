    "locator": "(//div/section[1]/div)[2]/div[2]",
    "type": "xpath"
  },
  "jobURL": {
    "locator": "//*/tr[1]/td[1]/a",
    "type": "xpath"
  },
  "pageJobsCount": {
    "locator": "//*[@id=\"resultCount\"]",
    "type": "xpath",
    "hasCountInText": "true"
  },
  "nextPage": {
    "locator": "//li[3]/span/a",
    "type": "xpath"
  }
